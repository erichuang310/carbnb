<!-- dates -->
<div class="row row-offset-6">
  <div class="col-xs-3">
    <label>Dates</label>
  </div>
  <div class="col-xs-9">
    <div class="row">
      <div class="col-xs-6">
        <input
          type="text"
          class="datepicker form-control"
          placeholder="Start Date"
          name="start_date">
      </div>
      <div class="col-xs-6">
        <input
          type="text"
          class="datepicker form-control"
          placeholder="End Date"
          name="end_date">
      </div>
    </div>
  </div>
</div>

<!-- car type -->
<div class="row row-offset-6">
    <div class="col-xs-3">
      <label>Car Type</label>
    </div>
  <div class="col-xs-9">
    <div class="row">
      <div class="col-xs-3">
        <label class="checkbox-inline">
          <input
            type="checkbox"
            name="car_type"
            value="sport">
            Sport
        </label>
      </div>
      <div class="col-xs-3">
        <label class="checkbox-inline">
          <input
            type="checkbox"
            name="car_type"
            value="economy">Economy
        </label>
      </div>
      <div class="col-xs-3">
        <label class="checkbox-inline">
          <input
            type="checkbox"
            name="car_type"
            value="luxury">
            Luxury
        </label>
      </div>
      <div class="col-xs-3">
        <label class="checkbox-inline">
        <input
          type="checkbox"
          name="car_type"
          value="utility">
          Utility
        </label>
      </div>
    </div>
  </div>
</div>



<!-- car type -->
<div class="row row-offset-6">
  <div class="col-xs-3">
    <label>Price Range</label>
  </div>
  <div class="col-xs-9">
    <div class="row">
      <div class="col-xs-1" id="min-amount">
        $50
      </div>
      <div class="col-xs-10">
        <div id="slider-range"></div>
      </div>
      <div class="col-xs-1" id="max-amount">
        $100
      </div>
    </div>
  </div>
</div>


<script>
  $(function() {
    $( "#slider-range" ).slider({
      range: true,
      min: 50,
      max: 500,
      values: [ 75, 100 ],
      slide: function( event, ui ) {
        $("#min-amount").html("$" +  ui.values[ 0 ]);
        $("#max-amount").html("$" +  ui.values[ 1 ]);
      }
    });
    $( "#amount" ).val( "$" + $( "#slider-range" ).slider( "values", 0 ) +
      " - $" + $( "#slider-range" ).slider( "values", 1 ) );
  });
</script>
