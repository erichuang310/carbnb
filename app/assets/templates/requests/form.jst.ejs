<div class="list-group-item" id="new-request-title">
  <h4>Make a request</h3>
</div>

<div class="list-group-item" id="new-request-title">
  <form>
    <div id="flash-message"></div>
    <div class="row row-offset-4">
      <div class="col-xs-6">
        <label for="request-start-date">Start Date</label>
      </div>
      <div class="col-xs-6">
        <label for="request-end-date">End Date</label>
      </div>
    </div>
    <div class="row row-offset-4">
      <div class="col-xs-6">
        <input
          type="text"
          id="request-start-date"
          class="form-control date-picker"
          placeholder="mm/dd/yy"
          name="request[start_date]">
      </div>

      <div class="col-xs-6">
        <input
          type="text"
          id="request-end-date"
          class="form-control date-picker"
          placeholder="mm/dd/yy"
          name="request[end_date]">
      </div>
    </div>

    <div class="row row-offset-4">
      <div class="col-xs-12">
        <input
          type="submit"
          value="Reserve"
          class="form-control">
      </div>
    </div>
  </form>
</div>
