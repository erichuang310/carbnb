<div id="content" class="container-fluid"></div>

<script>

  <% if current_user %>
    Carbnd.currentUser = new Carbnd.Models.User({
      name: "<%= current_user.name %>",
      email: "<%= current_user.email %>"
    });
  <% else %>
    Carbnd.currentUser = new Carbnd.Models.User();
  <% end %>

  $(document).ready(function(){
    Carbnd.initialize();
  });
</script>
